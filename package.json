{
  "name": "jotai-devtools",
  "version": "0.1.0",
  "description": "A powerful toolkit to enhance your development experience with Jotai",
  "author": {
    "name": "Arjun Vegda",
    "url": "https://arjunvegda.com"
  },
  "sideEffects": false,
  "source": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.esm.mjs"
      },
      "module": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.esm.mjs"
      },
      "module": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.esm.js"
      },
      "default": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.cjs.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "test:coverage": "yarn run test --coverage",
    "test:ci": "yarn run test:coverage --maxWorkers=1",
    "commitlint": "commitlint",
    "lint-staged": "lint-staged",
    "precompile": "rimraf dist",
    "compile": "rollup -c rollup.config.ts --configPlugin typescript",
    "postcompile": "node -e \"require('shelljs').find('dist/**/*.d.ts').forEach(f=>require('fs').copyFileSync(f,f.replace(/\\.ts$/,'.mts')))\"; &> /dev/null shx sed -i \"s/ from '(\\..*)';$/ from '\\$1.mjs';/\" 'dist/**/*.d.mts'; shx sed -i \"s/^declare module '(\\..*)'/declare module '\\$1.mjs'/\" 'dist/**/*.d.mts' &> /dev/null"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jotai-labs/jotai-devtools.git"
  },
  "keywords": [
    "jotai",
    "devtools",
    "debugger",
    "react",
    "state",
    "management",
    "redux devtools"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jotai-labs/jotai-devtools/issues"
  },
  "homepage": "https://github.com/jotai-labs/jotai-devtools",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/helper-annotate-as-pure": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.20.7",
    "@babel/plugin-transform-typescript": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/template": "^7.20.7",
    "@babel/types": "^7.20.7",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.25",
    "@swc/jest": "^0.2.24",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.6.0",
    "esbuild-plugin-replace": "^1.3.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "jotai": "^1.12.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux": "^4.2.0",
    "rimraf": "^4.0.4",
    "rollup": "^3.10.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-transform-paths": "^3.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "jotai": ">=1.8.6 <2.x",
    "react": ">=16.8"
  },
  "dependencies": {
    "@redux-devtools/extension": "^3.2.3"
  }
}
